<div class="layout-978">
	
	<div class="row">
		<div class="col6">
		<a name="hackernews" />
		<br/>
		<ul>
			<h3 id="h3">Hacker News</h3>
		 <% size=@hacker.all.size%>
		 <% for i in (1..16)%>
		 <li>
		 <a href="<%= @hacker.find(i).link %>"> :: <%= @hacker.find(i).name  %></a>	
		 </li>
		 <% end %>
		</ul>
		</div>

		<div class="col6">
		<br/><br/><br/>
		<ul>
	    <h3></h3>
		<% size=@hacker.all.size%>
		<% for i in (17..size-1)%>
		  <li>
		  <a class="links" href="<%= @hacker.find(i).link %>"> :: <%= @hacker.find(i).name  %></a>
		  </li>	
		<% end %>
		</ul>		
		</div>
	<div class="row-end">&nbsp;</div>
	</div>		
	
	
